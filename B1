import haversine 

cA={"city" : "larisa","population" : 128760,"latitude" : 39.63722220,"longitude" : 22.42027780}
cB={"city" : "ioannina","population" : 64012,"latitude" : 39.66750000,"longitude" : 20.85083330}   
#dimiourgia antikeimenou kai apothikeusi tou sti metavliti distance
distance=haversine.getDistanceInKm(cA["latitude"],cA["longitude"],cB["latitude"],cB["longitude"])
print("Η απόσταση μεταξύ",cA["city"]," και", cB["city"],"είναι ",distance," km")
